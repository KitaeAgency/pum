<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="pum.type.price" class="Pum\Bundle\TypeExtraBundle\Pum\Type\PriceType">
            <tag name="pum.type" alias="price" />
        </service>

        <service id="pum.type.coordinate" class="Pum\Bundle\TypeExtraBundle\Pum\Type\CoordinateType">
            <tag name="pum.type" alias="coordinate" />
        </service>

        <service id="pum.type.media" class="Pum\Bundle\TypeExtraBundle\Pum\Type\MediaType">
            <tag name="pum.type" alias="media" />
        </service>

        <service id="pum.type.html" class="Pum\Bundle\TypeExtraBundle\Pum\Type\HtmlType">
            <tag name="pum.type" alias="html" />
        </service>

    </services>
</container>
