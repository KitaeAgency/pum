{% extends "PumWoodworkBundle::layout.html.twig" %}

{% set menu_active = "users" %}

{% block content %}
    <div class="pum-row-head">
        {{ pum_macros.section_title('Users', 'Manage application users', null, [
            {
                href: path('ww_homepage'),
                text: 'Dashboard'
            },
            {
                href: path('ww_user_list'),
                text: 'Beams'
            }
        ]) }}
    </div>
    <div class="row">
        <div class="col-lg-4 col-offset-auto">
            {{form(form)}}
        </div>
    </div>
{% endblock %}

{% block javascript %}
    <script>
        var objects = [];
        {% for object in objects %}
            objects.push('{{ object }}');
        {% endfor %}
        $( "#ww_relation_from" ).autocomplete({
         source: objects
        });
        $( "#ww_relation_to" ).autocomplete({
         source: objects
        });
    </script>
{% endblock %}
