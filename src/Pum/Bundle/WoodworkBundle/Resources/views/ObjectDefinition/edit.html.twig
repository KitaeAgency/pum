{% extends "PumWoodworkBundle::layout.html.twig" %}

{% block content %}
<h2>Edit Definition</h2>
<p><a href="{{ path('ww_object_definition_edit', {name: objectName, beamName: beam.name}) }}">Save object</a></p>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Beam</th>
            <th>Object</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td> {{ beam.name }} </td>
            <td> {{ objectName }}</td>
        </tr>
    </tbody>
</table>
{% endblock %}
