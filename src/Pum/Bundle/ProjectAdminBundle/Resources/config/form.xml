<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">


    <services>

        <service id="form.type.pa_tableview" class="Pum\Bundle\ProjectAdminBundle\Form\Type\TableViewType">
            <tag name="form.type" alias="pa_tableview" />
        </service>

        <service id="form.type.pa_tableview_columns" class="Pum\Bundle\ProjectAdminBundle\Form\Type\TableViewColumnsType">
            <tag name="form.type" alias="pa_tableview_columns" />
        </service>

        <service id="form.type.pa_tableview_sort" class="Pum\Bundle\ProjectAdminBundle\Form\Type\TableViewSortType">
            <tag name="form.type" alias="pa_tableview_sort" />
        </service>

        <service id="form.type.pa_tableview_filters" class="Pum\Bundle\ProjectAdminBundle\Form\Type\TableViewFiltersType">
            <tag name="form.type" alias="pa_tableview_filters" />
        </service>

        <service id="form.type.pa_objectview" class="Pum\Bundle\ProjectAdminBundle\Form\Type\ObjectViewType">
            <tag name="form.type" alias="pa_objectview" />
        </service>

        <service id="form.type.pa_objectview_columns" class="Pum\Bundle\ProjectAdminBundle\Form\Type\ObjectViewColumnsType">
            <tag name="form.type" alias="pa_objectview_columns" />
        </service>

        <service id="form.type.pa_formview" class="Pum\Bundle\ProjectAdminBundle\Form\Type\FormViewType">
            <tag name="form.type" alias="pa_formview" />
        </service>

        <service id="form.type.pa_formview_columns" class="Pum\Bundle\ProjectAdminBundle\Form\Type\FormViewColumnsType">
            <tag name="form.type" alias="pa_formview_columns" />
        </service>

        <service id="form.type.pum_filter" class="Pum\Extension\ProjectAdmin\Form\Type\PumFilterType">
            <argument type="service" id="pum" />
            <tag name="form.type" alias="pum_filter" />
        </service>

    </services>
</container>
